{
	"name": "westminster-svg",
	"description": "Generate westminster parliament charts as virtual-dom SVG.",
	"version": "1.1.2",
	"keywords": [
		"parliament",
		"westminster",
		"house",
		"commons",
		"house of commons",
		"election",
		"wiki",
		"wikipedia",
		"parlament",
		"chart",
		"svg"
	],
	"author": "Julius Tens <mail@juliustens.eu>",
	"homepage": "https://github.com/juliuste/westminster-svg",
	"repository": "juliuste/westminster-svg",
	"bugs": "https://github.com/juliuste/westminster-svg/issues",
	"files": [
		"src/*"
	],
	"main": "src/index.js",
	"module": "src/index.js",
	"dependencies": {
		"lodash": "^4.17.21",
		"virtual-hyperscript-svg": "^2.0.0"
	},
	"devDependencies": {
		"browserify": "^17.0.0",
		"depcheck": "^1.4.2",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"tape": "^5.3.1",
		"uglify-js": "^3.14.2",
		"virtual-dom": "^2.1.1",
		"virtual-dom-stringify": "^3.0.1"
	},
	"scripts": {
		"build": "npm run bundle && npm run minify",
		"bundle": "browserify docs/index.js > docs/bundle/index.js",
		"minify": "uglifyjs -mc -- docs/bundle/index.js > docs/bundle/index.min.js",
		"check-deps": "depcheck --ignores='webpack-cli' --ignore-dirs='bundle'",
		"fix": "npm run lint -- --fix",
		"lint": "eslint src test example docs/index.js",
		"prepublishOnly": "npm test",
		"test": "npm run lint && npm run check-deps && tape test/*.js"
	},
	"license": "ISC",
	"engines": {
		"node": ">=14"
	}
}
