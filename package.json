{
	"name": "westminster-svg",
	"description": "Generate westminster parliament charts as virtual-dom SVG.",
	"version": "1.1.2",
	"keywords": [
		"parliament",
		"westminster",
		"house",
		"commons",
		"house of commons",
		"election",
		"wiki",
		"wikipedia",
		"parlament",
		"chart",
		"svg"
	],
	"author": "Julius Tens <mail@juliustens.eu>",
	"homepage": "https://github.com/juliuste/westminster-svg",
	"repository": "juliuste/westminster-svg",
	"bugs": "https://github.com/juliuste/westminster-svg/issues",
	"main": "./index.js",
	"module": "./index.js",
	"files": [
		"index.js"
	],
	"dependencies": {
		"lodash": "^4.17.21",
		"virtual-hyperscript-svg": "^2.0.0"
	},
	"devDependencies": {
		"browserify": "^17.0.0",
		"svgson": "^5.2.1",
		"tape": "^5.3.1",
		"uglify-js": "^3.14.2",
		"virtual-dom": "^2.1.1",
		"virtual-dom-stringify": "^3.0.1"
	},
	"scripts": {
		"test": "tape test/*.js",
		"bundle": "browserify docs/index.js > docs/bundle/index.js",
		"minify": "uglifyjs -mc -- docs/bundle/index.js > docs/bundle/index.min.js",
		"build": "npm run bundle && npm run minify",
		"prepublishOnly": "npm run test"
	},
